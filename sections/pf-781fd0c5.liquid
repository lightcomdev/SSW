<script>
  document.documentElement.classList.add('js');

var mslider = document.querySelector("#desired-mix-percentage");
mslider.addEventListener('input', e => {
    let _t = e.target;
    _t.parentNode.style.setProperty('--val', +_t.value);

    var n=_t.value;
    if (_t.value == 12) {
      n = 12.5
    }


    document.getElementById("Mix_input").innerHTML = "Desired Mix Percentage: " + n + "%";
}, false);


var mslider = document.querySelector("#bleach-strength");
mslider.addEventListener('input', e => {
    let _t = e.target;
    _t.parentNode.style.setProperty('--val', +_t.value);

    var n=_t.value;
    if (n == 1) {
      n = 6
    }else if (n == 2) {
      n = 10
    }else if (n == 3) {
      n = 12.5
    }


    document.getElementById("Bleach_input").innerHTML = "Bleach (SH) Strength: " + n + "%";
}, false);








// Event Listeners
document.getElementById("calculate").addEventListener("click", calculateMix);
document.getElementById("Liters").addEventListener("click", changeUnits);
document.getElementById("Gallons").addEventListener("click", changeUnits);

function calculateMix() {

    // Declare and assign variables
    let waterOutput;
    let bleachOutput;
    let surfactantOutput;
    let bleachOutputUnits;
    let surfactantOutputUnits;
    let units = document.querySelector('input[name="radio"]:checked').value;
    let desiredMixAmount = Number(document.getElementById("desired-mix-amount").value.replace(/%/g, ""));
    let desiredMixPercentage = Number(document.getElementById("desired-mix-percentage").value.replace(/%/g, ""));
    let bleachStrength = Number(document.getElementById("bleach-strength").value.replace(/%/g, ""));
    let surfactantAmount = Number(document.getElementById("surfactant-amount").value.replace(/%/g, ""));

    // Set Output Units
    if (units == "Liters") {
        bleachOutputUnits = "Liters";
        surfactantOutputUnits = "Milliliters";
    } else {
        bleachOutputUnits = "Gallons";
        surfactantOutputUnits = "Ounces";
    }


    if (desiredMixAmount == 12) {
      desiredMixAmount = 12.5
    }

    if (bleachStrength == 1) {
      bleachStrength = 6
    }else if (bleachStrength == 2) {
      bleachStrength = 10
    }else if (bleachStrength == 3) {
      bleachStrength = 12.5
    }

    

    // Error Handling
    if (desiredMixPercentage > bleachStrength) {
        errorHandling();
        return;
    }
    document.getElementById("isvalidcal").style.display = "block";
    document.getElementById("isnotvalidcal").style.display = "none";



    // Calculations
    bleachOutput = (desiredMixPercentage / bleachStrength * desiredMixAmount).toFixed(1);
    waterOutput = (desiredMixAmount - bleachOutput).toFixed(1);
    //surfactantOutput = (surfactantAmount * desiredMixAmount).toFixed(0);
    surfactantOutput = (surfactantAmount).toFixed(0);

    // Output
    document.getElementById("water-output").innerHTML = waterOutput + " <span>" + bleachOutputUnits + "</span>";
    document.getElementById("bleach-output").innerHTML = bleachOutput + " <span>" + bleachOutputUnits + "</span>";
    document.getElementById("surfactant-output").innerHTML = surfactantOutput + " <span>" + surfactantOutputUnits + "</span>";

}
calculateMix();

function changeUnits() {
    document.querySelector(".mycalculator").classList.toggle("liters_system");
}

function errorHandling() {
    document.getElementById("isvalidcal").style.display = "none";
    document.getElementById("isnotvalidcal").style.display = "block";
}
document.querySelectorAll(".__range-step").forEach(function(ctrl) {
  var el = ctrl.querySelector('input');        
  var output = ctrl.querySelector('output'); 
  var newPoint, newPlace, offset;
  el.oninput =function(){ 
    // colorize step options
    ctrl.querySelectorAll("option").forEach(function(opt) {
      if(opt.value<=el.valueAsNumber)                
        opt.style.backgroundColor = 'black';
      else
        opt.style.backgroundColor = '#aaa';
    });           
    // colorize before and after
    var valPercent = (el.valueAsNumber  - parseInt(el.min)) / (parseInt(el.max) - parseInt(el.min));            
    var style = 'background-image: -webkit-gradient(linear, 0% 0%, 100% 0%, color-stop('+
    valPercent+', black), color-stop('+
    valPercent+', #aaa));';
    el.style = style;

    // Popup
    if((' ' + ctrl.className + ' ').indexOf(' ' + '__range-step-popup' + ' ') > -1)
    {
      var selectedOpt=ctrl.querySelector('option[value="'+el.value+'"]');
      output.innerText= selectedOpt.text;
      output.style.left = "50%"; 
      output.style.left = ((selectedOpt.offsetLeft + selectedOpt.offsetWidth/2) - output.offsetWidth/2) + 'px'; 
    }           
  };
  el.oninput();    
});

window.onresize = function(){
  document.querySelectorAll(".__range").forEach(function(ctrl) {
    var el = ctrl.querySelector('input');    
    el.oninput();    
  });
};
